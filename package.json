    "mocha": "^4.1.0",
    "mocha-jscs": "^5.0.1",
    "mocha-jshint": "^2.3.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "auth0": "^2.9.1",
    "auth0-js": "^8.12.1",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
